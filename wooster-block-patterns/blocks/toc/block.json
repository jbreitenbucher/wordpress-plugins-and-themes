{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wbp/toc",
  "title": "Table of Contents",
  "description": "Automatically lists headings on the page with working links and proper indentation.",
  "textdomain": "wooster-block-patterns",
  "category": "wbp-content",
  "icon": "list-view",
  "supports": {
    "html": false,
    "anchor": false,
    "align": [ "wide", "full" ],
    "color": { "background": true, "text": true },
    "spacing": { "padding": true, "margin": true }
  },
  "attributes": {
    "title": {
      "type": "string",
      "default": "On this page"
    },
    "collapsed": {
      "type": "boolean",
      "default": false
    },
    "maxDepth": {
      "type": "number",
      "default": 3
    }
  }
}